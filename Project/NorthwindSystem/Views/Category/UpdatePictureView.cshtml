@model NorthwindSystem.Data.DTOModels.CategoryDto

<div>
    <a asp-action="Index">Back to Category List</a>
</div>

<h2>@ViewBag.Method for @Model.Name</h2>

<form method="post" enctype="multipart/form-data" asp-action="Update", asp-route-categoryId="@Model.Id">
    <div class="form-group">
        <div class="col-md-10">
            <input type="file" name="picture" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Save" />
        </div>
    </div>
</form>